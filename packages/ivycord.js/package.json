{
  "name": "ivycord.js",
  "version": "0.0.1",
  "description": "A powerful Discord API wrapper JavaScript library.",
  "license": "MIT",
  "homepage": "https://ivycord.js.org",
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "check-format": "prettier . --check",
    "run-format": "prettier . --write"
  },
  "contributors": [
    "null68",
    "kikorp78",
    "camdzic",
    "spectral37"
  ],
  "keywords": [
    "discord",
    "api",
    "wrapper",
    "node",
    "ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ivycord-js/ivycord.js.git",
    "directory": "packages/ivycord.js"
  },
  "bugs": {
    "url": "https://github.com/ivycord-js/ivycord.js/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.14.0",
  "dependencies": {
    "axios": "^1.6.5",
    "discord-api-types": "^0.37.67",
    "ws": "^8.16.0",
    "zlib-sync": "^0.1.9"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  }
}
